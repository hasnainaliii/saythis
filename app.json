{
  "expo": {
    "name": "saythis",
    "slug": "saythis",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/saythis-high-resolution-logo.png",
    "scheme": "saythis",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/saythis-high-resolution-logo.png",
        "backgroundImage": "./assets/images/saythis-high-resolution-logo.png",
        "monochromeImage": "./assets/images/saythis-high-resolution-logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.hasnainalii.saythis"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-Screen-saythis.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#4566",
          "dark": {
            "backgroundColor": "#4544"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
